{
  "name": "next-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-sitemap --config sitemap.config.js",
    "start": "next start",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -l -c fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "test": "run-p -l -c --aggregate-output lint:* unit",
    "unit": "jest",
    "storybook": "start-storybook -p 6006 -s ./mocks/data",
    "build-storybook": "build-storybook"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "0.3.0",
    "@tailwindcss/line-clamp": "0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "date-fns": "2.25.0",
    "microcms-js-sdk": "2.0.0",
    "next": "12.0.2",
    "next-pwa": "5.4.0",
    "next-seo": "4.28.1",
    "next-themes": "^0.0.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.3",
    "react-icons": "^4.3.1",
    "react-share": "4.4.0",
    "react-slick": "0.28.1",
    "react-switch": "^6.0.0",
    "react-use": "17.3.1",
    "remark-breaks": "3.0.1",
    "remark-emoji": "3.0.1",
    "remark-footnotes": "4.0.1",
    "remark-slug": "7.0.0",
    "remark-unwrap-images": "3.0.0",
    "slick-carousel": "1.8.1",
    "swr": "^1.0.0",
    "tocbot": "4.16.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@next/bundle-analyzer": "12.0.2",
    "@storybook/addon-a11y": "6.3.8",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "^6.3.8",
    "@swc/core": "1.2.112",
    "@swc/jest": "0.2.4",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/gtag.js": "^0.0.7",
    "@types/jest": "27.0.1",
    "@types/mdx-js__react": "1.5.4",
    "@types/node": "16.11.6",
    "@types/react": "17.0.20",
    "@types/react-slick": "0.23.7",
    "@types/react-test-renderer": "17.0.1",
    "@types/testing-library__react": "10.2.0",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "8.2.3",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tailwindcss": "1.17.0",
    "husky": "7.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "11.2.6",
    "msw": "0.35.0",
    "next-mdx-remote": "^3.0.4",
    "next-sitemap": "1.6.168",
    "npm-run-all": "4.1.5",
    "postcss": "^8.3.6",
    "prettier": "2.4.0",
    "react-test-renderer": "17.0.2",
    "storybook-addon-performance": "0.16.1",
    "storybook-tailwind-dark-mode": "1.0.11",
    "tailwindcss": "^2.2.14",
    "typescript": "4.4.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn fix:eslint",
    "*": "yarn fix:prettier"
  }
}
