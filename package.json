{
  "name": "next-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-sitemap --config sitemap.config.js",
    "start": "next start",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -l -c fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "test": "run-p -l -c --aggregate-output lint:* unit e2e",
    "unit": "jest",
    "e2e": "playwright test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
    "@headlessui/react": "1.6.4",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/line-clamp": "0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "date-fns": "2.28.0",
    "microcms-js-sdk": "2.0.0",
    "next": "12.1.6",
    "next-mdx-remote": "4.0.3",
    "next-pwa": "5.5.2",
    "next-seo": "5.4.0",
    "next-themes": "^0.2.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-error-boundary": "3.1.4",
    "react-icons": "^4.3.1",
    "react-share": "4.4.0",
    "react-slick": "0.29.0",
    "react-switch": "^6.0.0",
    "remark-breaks": "3.0.2",
    "remark-emoji": "3.0.2",
    "remark-footnotes": "4.0.1",
    "remark-slug": "7.0.1",
    "remark-unwrap-images": "3.0.1",
    "slick-carousel": "1.8.1",
    "swr": "^1.3.0",
    "tocbot": "4.18.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@next/bundle-analyzer": "12.1.6",
    "@playwright/test": "1.22.2",
    "@storybook/addon-a11y": "6.5.5",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.5.5",
    "@storybook/manager-webpack5": "6.5.5",
    "@storybook/react": "^6.5.5",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/react-hooks": "8.0.0",
    "@testing-library/user-event": "14.2.0",
    "@types/gtag.js": "^0.0.10",
    "@types/jest": "27.5.1",
    "@types/marked": "4.0.3",
    "@types/mdx-js__react": "1.5.5",
    "@types/node": "17.0.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "18.0.5",
    "@types/react-slick": "0.23.8",
    "@types/react-test-renderer": "18.0.0",
    "@types/testing-library__react": "10.2.0",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "8.2.5",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tailwindcss": "3.5.0",
    "feed": "4.2.2",
    "husky": "8.0.0",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "lint-staged": "12.4.1",
    "marked": "4.0.15",
    "msw": "0.41.1",
    "next-sitemap": "2.5.20",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "react-test-renderer": "18.1.0",
    "storybook-addon-next": "1.6.6",
    "storybook-addon-performance": "0.16.1",
    "storybook-tailwind-dark-mode": "1.0.12",
    "tailwindcss": "^3.0.24",
    "typescript": "4.7.2"
